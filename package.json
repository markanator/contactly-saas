{
	"name": "contactly-saas",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "pnpx playwright test",
		"test:unit": "vitest",
		"test:e2e": "pnpx playwright test",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen:types": "pnpx supabase gen types typescript --local > src/lib/supabase-types.ts && prettier --write src/lib/supabase-types.ts",
		"seed": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts",
		"stripe:webhooks": "stripe listen --forward-to localhost:5173/api/stripe/webhook"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"@supabase/auth-helpers-sveltekit": "^0.10.7",
		"@supabase/supabase-js": "^2.38.5",
		"clsx": "^2.0.0",
		"dotenv": "^16.3.1",
		"flowbite": "^2.2.0",
		"flowbite-svelte": "^0.44.20",
		"flowbite-svelte-icons": "^0.4.5",
		"stripe": "^14.5.0",
		"svelte-french-toast": "1.2.0"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.3.1",
		"@playwright/test": "^1.40.0",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-vercel": "^3.1.0",
		"@sveltejs/kit": "^1.27.6",
		"@types/detect-port": "^1.3.5",
		"@types/node": "^20.10.0",
		"@types/pg": "^8.10.9",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"autoprefixer": "^10.4.16",
		"detect-port": "^1.5.1",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"pg": "^8.11.3",
		"postcss": "^8.4.31",
		"postcss-load-config": "^4.0.2",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.2",
		"stripe-event-types": "^3.1.0",
		"supabase": "^1.113.3",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.2",
		"svelte-preprocess": "^5.1.1",
		"sveltekit-superforms": "^1.10.2",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"tsx": "^4.5.0",
		"typescript": "^5.3.2",
		"vite": "^4.5.2",
		"vitest": "^0.34.6",
		"zod": "^3.22.4"
	}
}